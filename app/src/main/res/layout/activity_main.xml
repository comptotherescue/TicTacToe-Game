<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity"
    android:id="@+id/ak"
    android:background="@drawable/images"
    android:focusable="true"
    >

    <TextView android:text="@string/hello_world" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/b1"
        android:layout_centerVertical="true"
        android:layout_toLeftOf="@+id/b2"
        android:layout_toStartOf="@+id/b2"
        android:background="@drawable/button_border"
        android:textColor="#ff000000"
        android:textColorHighlight="@android:color/black" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/b2"
        android:layout_above="@+id/b5"
        android:layout_centerHorizontal="true"
        android:background="@drawable/button_border"
        android:textColorHighlight="@android:color/black"
        android:textColor="#ff000000" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/b3"
        android:layout_above="@+id/b6"
        android:layout_toRightOf="@+id/b2"
        android:layout_toEndOf="@+id/b2"
        android:background="@drawable/button_border"
        android:textColorHighlight="@android:color/black"
        android:textColor="#ff000000" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/b4"
        android:layout_toLeftOf="@+id/b5"
        android:layout_toStartOf="@+id/b5"
        android:layout_below="@+id/b1"
        android:background="@drawable/button_border"
        android:textColorHighlight="@android:color/black"
        android:textColor="#ff000000" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/b5"
        android:layout_alignTop="@+id/b4"
        android:layout_alignLeft="@+id/b2"
        android:layout_alignStart="@+id/b2"
        android:background="@drawable/button_border"
        android:textColorHighlight="@android:color/black"
        android:textColor="#ff000000" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/b6"
        android:layout_alignBottom="@+id/b5"
        android:layout_alignLeft="@+id/b3"
        android:layout_alignStart="@+id/b3"
        android:background="@drawable/button_border"
        android:textColorHighlight="@android:color/black"
        android:textColor="#ff000000" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/b7"
        android:layout_below="@+id/b4"
        android:layout_toLeftOf="@+id/b8"
        android:layout_toStartOf="@+id/b8"
        android:background="@drawable/button_border"
        android:textColorHighlight="@android:color/black"
        android:textColor="#ff000000" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/b8"
        android:layout_alignBottom="@+id/b7"
        android:layout_alignLeft="@+id/b5"
        android:layout_alignStart="@+id/b5"
        android:background="@drawable/button_border"
        android:textColorHighlight="@android:color/black"
        android:textColor="#ff000000" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/b9"
        android:layout_alignBottom="@+id/b8"
        android:layout_alignLeft="@+id/b6"
        android:layout_alignStart="@+id/b6"
        android:background="@drawable/button_border"
        android:textColorHighlight="@android:color/black"
        android:textColor="#ff000000" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Reset"
        android:id="@+id/b10"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/textView"
        android:layout_toEndOf="@+id/textView" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:id="@+id/t2"
        android:layout_below="@+id/textView"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="55dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Welcome to TicTacToe"
        android:id="@+id/t1"
        android:layout_alignTop="@+id/textView"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
